abbr "cs"="docker compose stop"
abbr "cup"="docker compose up -d"
abbr "ga"="git add"
abbr "gb"="git branch"
abbr "gc"="git commit"
abbr "gco"="git checkout"
abbr "gd"="git diff"
abbr "gk"="git cherry-pick"
abbr "gl"="git lg"
abbr "gp"="git pull origin \$(current_branch)"
abbr "gP"="git push origin \$(current_branch)"
abbr "gr"="git remote -v"
abbr "grmu"="git status -s | awk '{ print  }' | xargs rm"
abbr "gss"="git status -s"
abbr "gu"="git status -s | grep UU"
abbr "gU"="git reset --soft HEAD~"
abbr "hw"="echo hello world"
abbr "labs"="cd ~/Projects/labs"
